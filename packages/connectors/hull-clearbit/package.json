{
  "name": "hull-clearbit",
  "description": "Enrich Customer data with Clearbit",
  "version": "0.0.17-monorepo",
  "license": "MIT",
  "homepage": "https://github.com/hull/hull-connectors",
  "keywords": [
    "hull",
    "ship",
    "hull-ship",
    "clearbit",
    "enrichment",
    "sync"
  ],
  "scripts": {
    "dev": "builder run dev",
    "test": "npm run test:lint && npm run test:modules && npm run test:coverage && npm run test:client && npm run test:units && npm run test:specs",
    "test:modules": "npm outdated --depth=0",
    "test:coverage": "NODE_ENV=test jest --env=jsdom --config=jest.config.json --coverage",
    "test:client": "NODE_ENV=test jest --env=jsdom --config=jest.config.json",
    "test:units": "NODE_ENV=test mocha --opts ./tests/mocha.opts ./tests/unit/*.js",
    "test:units:watch": "NODE_ENV=test mocha --watch --opts ./tests/mocha.opts ./tests/unit/*.js",
    "test:specs": "NODE_ENV=test mocha --opts ./tests/mocha.opts ./tests/integration/*.js",
    "test:specs:watch": "NODE_ENV=test mocha --watch --opts ./tests/mocha.opts ./tests/integration/*.js"


  },
  "dependencies": {
    "bottleneck": "^2.0.1",
    "clearbit": "^1.3.3",
    "object-mapper": "^4.2.0",
    "range_check": "^1.4.0",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-logger": "^3.0.6",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0",
    "request": "^2.83.0",
    "select2": "^4.0.6-rc.1",
    "sinon": "^4.2.0"
  },
  "devDependencies": {
    "updtr": "^2.0.0"
  }
}
