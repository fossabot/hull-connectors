{
  "name": "Tester",
  "description": "Send test data to Hull to test your flows",
  "tags": [],
  "logo": "logo.png",
  "picture": "picture.png",
  "ui": false,
  "tabs": [
    {
      "title": "Workspace",
      "url": "/admin.html",
      "size": "large",
      "setup": false,
      "workspace": true,
      "editable": true,
      "handler": "admin"
    }
  ],
  "readme": "readme.md",
  "version": "0.1.11-monorepo",
  "private_settings": [],
  "resources": [],
  "settings_sections": [],
  "json": [
    {
      "url": "/config",
      "handler": "configHandler",
      "options": {
        "cacheContextFetch": false
      }
    },
    {
      "url": "/entries",
      "handler": "entityListHandler",
      "options": {
        "cacheContextFetch": false,
        "respondWithError": true
      }
    },
    {
      "url": "/call",
      "handler": "callHandler",
      "options": {
        "cacheContextFetch": false
      }
    },
    {
      "url": "/preview",
      "handler": "previewHandler",
      "options": {
        "cacheContextFetch": false
      }
    }
  ],
  "subscriptions": [
    {
      "url": "/smart-notifier",
      "conditions": {
        "channels": {
          "only": [
            "ship:update"
          ]
        }
      },
      "channels": []
    }
  ],
  "status": {
    "initial_value": "ok",
    "url": "/status",
    "type": "cron",
    "value": "*/30 * * * *",
    "handler": "statusHandler"
  },
  "statuses": [
    {
      "initial_value": "ok",
      "url": "/status",
      "type": "cron",
      "value": "*/30 * * * *",
      "handler": "statusHandler"
    }
  ]
}
