    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "newrelic": "^4.9.0",
    "progress-bar-webpack-plugin": "^1.11.0",
    "react": "^16.8.3",
    "react-ace": "^6.4.0",
    "react-bootstrap": "1.0.0-beta.5",
    "react-dom": "^16.8.3",
    "raven": "^2.6.4",
