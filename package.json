{
  "name": "hull-connectors",
  "version": "0.1.36-monorepo",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/connectors/*",
      "packages/hull-connector-framework",
      "packages/hullrepl",
      "packages/hull-vm",
      "packages/hull-webhooks",
      "packages/hull",
      "packages/hull-client",
      "packages/minihull"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "pretty-quick --staged",
      "eslint packages --fix",
      "git add"
    ]
  },
  "license": "MIT",
  "scripts": {
    "duplicated-dependencies": "syncpack-list-mismatches -s 'packages/*/package.json' -s 'packages/connectors/*/package.json'",
    "build": "sh scripts/build-connectors.sh",
    "repl": "NODE_ENV=development sh scripts/repl.sh",
    "clean": "sh scripts/clean-connectors.sh",
    "start": "PORT=8080 bash scripts/bash-entrypoint.sh",
    "start:lightweight": "PORT=8080 CONNECTOR_TYPE=lightweight bash scripts/bash-entrypoint.sh",
    "combined": "sh scripts/combined-start.sh",
    "combined:lightweight": "sh scripts/lightweight-start.sh",
    "build:client": "sh scripts/build-webpack-project.sh",
    "ngrok": "sh scripts/ngrok.sh",
    "dev": "NODE_ENV=development sh scripts/start-dev-connector.sh",
    "client": "NODE_ENV=development sh scripts/start-client-connector.sh",
    "run-test": "sh scripts/run-test.sh",
    "watch-test": "sh scripts/watch-test.sh",
    "test": "yarn test:lint && jest && yarn wsrun --parallel test",
    "test:flow": "flow check",
    "test:lint": "eslint packages",
    "test:jest": "jest --maxWorkers=2",
    "test:workspaces": "yarn wsrun --fast-exit --parallel --concurrency=2 test",
    "test:ci": "yarn test:lint && jest --maxWorkers=2 && yarn wsrun --fast-exit --parallel --concurrency=2 test",
    "heroku-postbuild": "yarn build",
    "heroku-prebuild": "rm -rf node_modules"
  },
  "engines": {
    "node": "^12",
    "npm": "6.4.x",
    "yarn": "^1.10.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/register": "^7.4.4",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-polyfill": "^6.26.0",
    "chai": "^4.2.0",
    "chai-subset": "^1.6.0",
    "documentation": "^11.0.1",
    "dotenv": "^8.2.0",
    "dotenv-cli": "^2.0.0",
    "file-loader": "^3.0.1",
    "flow-bin": "^0.101.0",
    "flow-typed": "^2.5.2",
    "jest": "^24.8.0",
    "mocha": "^6.1.4",
    "nock": "^13.0.4",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "react-hot-loader": "^4.11.1",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "syncpack": "^4.4.2",
    "webpack-dev-server": "^3.3.1",
    "webpack-hot-middleware": "^2.24.4"
  },
  "dependencies": {
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
