{
  "name": "hull-connectors",
  "version": "0.0.17-monorepo",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/connectors/*",
      "packages/*"
    ],
    "nohoist": [
      "**/hull",
      "**/hull-client",
      "**/minihull",
      "**/hull-repl",
      "**/hull-connector-framework"
    ]
  },
  "license": "MIT",
  "scripts": {
    "duplicated-dependencies": "syncpack-list-mismatches -s 'packages/*/package.json' -s 'packages/connectors/*/package.json'",
    "build": "sh scripts/build-connectors.sh",
    "clean": "sh scripts/clean-connectors.sh",
    "build:client": "sh scripts/build-webpack-project.sh",
    "ngrok": "sh scripts/ngrok.sh",
    "dev": "NODE_ENV=development sh scripts/start-dev-connector.sh",
    "test": "yarn test:lint && jest && yarn workspaces run test",
    "test:flow": "flow check",
    "test:lint": "eslint packages",
    "test:ci": "yarn test:lint && jest --runInBand && nyc --babel-cache --hook-run-in-context false yarn workspaces run test",
    "heroku-postbuild": "yarn build"
  },
  "engines": {
    "node": "^10.15.3",
    "npm": "6.4.x",
    "yarn": "^1.10.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-syntax-export-default-from": "^7.2.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.2.0",
    "@babel/plugin-transform-flow-strip-types": "^7.4.0",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.6.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-lodash": "^3.3.4",
    "chai": "^4.2.0",
    "chai-subset": "^1.6.0",
    "css-loader": "^2.1.1",
    "documentation": "^9.3.1",
    "dotenv-cli": "^2.0.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-import-resolver-webpack": "^0.11.0",
