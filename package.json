    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "flow-bin": "^0.96.0",
    "flow-typed": "^2.5.1",
    "glob": "^7.1.3",
    "jest": "^24.6.0",
    "lodash-webpack-plugin": "^0.11.5",
    "mini-css-extract-plugin": "^0.5.0",
    "mocha": "^6.0.2",
