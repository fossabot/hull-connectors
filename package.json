{
  "name": "hull-connectors",
  "version": "0.0.13-monorepo",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/connectors/*",
      "packages/*"
    ],
    "nohoist": [
      "**/hull",
      "**/hull-client",
      "**/minihull",
      "**/hull-repl",
      "**/hull-connector-framework"
    ]
  },
  "license": "MIT",
  "scripts": {
    "duplicated-dependencies": "syncpack-list-mismatches -s 'packages/*/package.json' -s 'packages/connectors/*/package.json'",
    "build": "sh scripts/build-connectors.sh",
    "clean": "sh scripts/clean-connectors.sh",
    "build:client": "sh scripts/build-webpack-project.sh",
    "ngrok": "sh scripts/ngrok.sh",
    "dev": "NODE_ENV=development sh scripts/start-dev-connector.sh",
    "test": "yarn test:lint && jest && yarn workspaces run test",
    "test:lint": "eslint packages",
    "test:ci": "yarn test:lint && jest --runInBand && nyc --babel-cache --hook-run-in-context false yarn workspaces run test",
    "heroku-postbuild": "yarn build"
  },
  "engines": {
    "node": "8.12.x",
    "npm": "6.4.x",
    "yarn": "^1.10.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.2.0",
    "@babel/node": "^7.0.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-flow-strip-types": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-lodash": "^3.3.4",
    "chai": "^4.2.0",
    "chai-subset": "^1.6.0",
    "css-loader": "^2.0.1",
    "documentation": "^8.1.2",
    "dotenv-cli": "^1.4.0",
    "eslint": "^5.10.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-flowtype-errors": "^3.6.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.25.0",
    "eslint-plugin-prettier": "^3.0.0",
    "flow-bin": "^0.82.0",
    "flow-typed": "^2.5.1",
    "glob": "^7.1.3",
    "jest": "^23.6.0",
    "lodash-webpack-plugin": "^0.11.5",
    "mini-css-extract-plugin": "^0.5.0",
    "mocha": "^6.0.2",
    "nock": "^10.0.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.7",
    "nyc": "^13.0.1",
    "prettier": "^1.14.3",
    "react-hot-loader": "^3.1.1",
    "reactotron-react-js": "^1.12.2",
    "reactotron-redux": "^1.12.2",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "sinon": "^6.3.5",
    "sinon-chai": "^3.2.0",
    "style-loader": "^0.23.1",
    "syncpack": "^3.0.0",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "dependencies": {
    "JSONStream": "^1.3.4",
